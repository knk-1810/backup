<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.websarva.wings.android.backup">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SoftwareEngineering_Project">
        <activity
            android:name="com.websarva.wings.android.backup.MainActivity"
            android:exported="true"
            android:configChanges="locale">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.websarva.wings.android.backup.LoginActivity"
            android:label="login"
            android:configChanges="locale">
        </activity>
        <activity
            android:name="com.websarva.wings.android.backup.PassAskActivity"
            android:label="passAsk"
            android:configChanges="locale">
        </activity>
        <activity
            android:name="com.websarva.wings.android.backup.PassUpdateActivity"
            android:label="passAskComp"
            android:configChanges="locale">
        </activity>
        <activity
            android:name="com.websarva.wings.android.backup.UserAddActivity"
            android:label="userAdd"
            android:configChanges="locale">
        </activity>
        <activity
            android:name="com.websarva.wings.android.backup.HomeActivity"
            android:label="home"
            android:configChanges="locale">
        </activity>
    </application>

</manifest>